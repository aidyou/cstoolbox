{
  "config": {
    "name": "Google",
    "url_template": "/search?q={kw}&ie=UTF-8&newwindow=1&start={offset}&num={number}",
    "pagination_type": "offset",
    "pagination_param": "offset",
    "wait_for": "#rso",
    "browser_type": "chromium",
    "headless": true,
    "page_timeout": 60000,
    "max_results_per_page": 25
  },
  "schema": {
    "base_selector": "div[data-hveid][data-ved].g > div[data-snc], .g > div[data-hveid][data-ved] > div[class]",
    "fields": [
      {
        "name": "site_name",
        "selector": "a > div > div > div > div > span",
        "type": "text"
      },
      {
        "name": "title",
        "selector": "h3",
        "type": "text"
      },
      {
        "name": "url",
        "selector": "a[href]",
        "type": "attribute",
        "attribute": "href"
      },
      {
        "name": "summary",
        "selector": "div[data-snf][data-sncf]  *",
        "type": "text"
      }
    ]
  }
}